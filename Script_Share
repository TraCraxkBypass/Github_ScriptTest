-- // ReGui Base
local ReGui = loadstring(game:HttpGet('https://raw.githubusercontent.com/depthso/Dear-ReGui/refs/heads/main/ReGui.lua'))()
local PrefabsId = `rbxassetid://{ReGui.PrefabsId}`

ReGui:Init({
	Prefabs = game:GetService("InsertService"):LoadLocalAsset(PrefabsId)
})

-- // Script Function
local Player = game.Players.LocalPlayer
local Character = Player.Character or Player.CharacterAdded:Wait()
local HumanoidRootPart = Character:WaitForChild("HumanoidRootPart")

-- // Window
local Window = ReGui:Window({
	Title = "RNG Anime Girl [Beta]",
	Size = UDim2.fromOffset(300, 200)
})

-- // Main
Window:Label({Text="Main"})
Window:Label({Text="Sell"})
Window:Button({
	Text = "Sell All",
	Callback = function()
		game:GatService("ReplicatedStorage")WaitForChild("CORE_RemoteEvents"):WaitForChild("SendSellRequrest"):FireServer("sell_all")
	end,
})

-- // Teleport
Window:Label({Text="Teleport"})
Window:Label({Text="Crystal Caves"})
Window:Button({
	Text = "Crystal Caves",
	Callback = function()
		HumanoidRootPart.CFrame = CFrame.new(Vector3.new(140, -85, -265))
	end,
})
Window:Label({Text="Flooded Caves"})
Window:Button({
	Text = "Flooded Caves",
	Callback = function()
		HumanoidRootPart.CFrame = CFrame.new(Vector3.new(150, -87, 10))
		wait(1)
		HumanoidRootPart.CFrame = CFrame.new(Vector3.new(20, -88, -25))
		wait(1)
		HumanoidRootPart.CFrame = CFrame.new(Vector3.new(170, -85, -62.5))
	end,
})
