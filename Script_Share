-- // ReGui Base
local ReGui = loadstring(game:HttpGet('https://raw.githubusercontent.com/depthso/Dear-ReGui/refs/heads/main/ReGui.lua'))()
local PrefabsId = `rbxassetid://{ReGui.PrefabsId}`

ReGui:Init({
	Prefabs = game:GetService("InsertService"):LoadLocalAsset(PrefabsId)
})

-- // Script Function
local Player = game.Players.LocalPlayer
local Character = Player.Character or Player.CharacterAdded:Wait()
local HumanoidRootPart = Character:WaitForChild("HumanoidRootPart")

-- // Teleport Part
local Crystal_Cave = workspace.MapFolder.Obby_CrystalCaves.Model.Model.Model.Part

-- // Window
local Window = ReGui:Window({
	Title = "RNG Anime Girl [Beta]",
	Size = UDim2.fromOffset(300, 200)
})

-- // Teleport
Window:Label({Text="Teleport"})
Window:Label({Text="Crystal Cave"})
Window:Button({
	Text = "Crystal Cave",
	Callback = function()
		HumanoidRootPart.CFrame = Crystal_Cave.CFrame
	end,
})
